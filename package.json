{
   "name": "streaming-client",
   "version": "1.0.0",
   "description": "sample streaming client for OHIP",
   "exports": "./build/run.js",
   "type": "module",
   "engines": {
      "node": ">=14.16"
   },
   "scripts": {
      "lint": "eslint -c .eslintrc --ext .ts ./src",
      "lint-fix": "eslint -c .eslintrc --ext .ts ./src --fix ",
      "postinstall": "npx tsc",
      "dev": "tsc-watch --onSuccess \"node ./build/run.js\"",
      "start": "npx tsc && node ./build/run.js",
      "debug-git": "npx tsc && GIT_TRACE_CURL=true DEBUG=simple-git:* node ./build/run.js"
   },
   "author": "Luis Weir",
   "license": "ISC",
   "dependencies": {
      "@types/node": "^18.11.0",
      "@types/ws": "^8.5.3",
      "dotenv": "^16.0.3",
      "got": "^12.5.2",
      "graphql-ws": "^5.11.2",
      "tslog": "^3.3.4",
      "ws": "^8.9.0"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.40.0",
      "@typescript-eslint/parser": "^5.40.0",
      "eslint": "^8.25.0",
      "ts-node": "^10.9.1",
      "tsc-watch": "^5.0.3",
      "typescript": "^4.8.4"
   }
}
