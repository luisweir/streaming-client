{
   "name": "streaming-client",
   "version": "1.0.0",
   "description": "sample streaming client for OHIP",
   "exports": "./build/run.js",
   "type": "module",
   "engines": {
      "node": ">=14.16"
   },
   "scripts": {
      "lint": "eslint -c .eslintrc --ext .ts ./src",
      "lint-fix": "eslint -c .eslintrc --ext .ts ./src --fix ",
      "postinstall": "npx tsc",
      "dev": "tsc-watch --onSuccess \"node ./build/app.js\"",
      "start": "npx tsc && node --max-old-space-size=8192 ./build/app.js"
   },
   "author": "Luis Weir",
   "license": "ISC",
   "dependencies": {
      "@types/node": "^18.11.0",
      "@types/ws": "^8.5.3",
      "dotenv": "^16.0.3",
      "got": "^12.5.2",
      "graphql-ws": "^5.11.2",
      "kafkajs": "^2.2.4",
      "npm": "^10.6.0",
      "tslog": "^3.3.4",
      "uuid": "9.0.0",
      "ws": "^8.9.0"
   },
   "devDependencies": {
      "@types/uuid": "8.3.4",
      "eslint": "^8.25.0",
      "ts-node": "^10.9.2",
      "tsc-watch": "^5.0.3",
      "typescript": "^4.9.5"
   }
}
