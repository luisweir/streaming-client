{
   "name": "streaming-client",
   "version": "1.0.0",
   "description": "sample streaming client for OHIP",
   "exports": "./build/run.js",
   "type": "module",
   "engines": {
      "node": ">=14.16"
   },
   "scripts": {
      "lint": "eslint -c .eslintrc --ext .ts ./src",
      "lint-fix": "eslint -c .eslintrc --ext .ts ./src --fix ",
      "postinstall": "npx tsc",
      "dev": "tsc-watch --onSuccess \"node ./build/run.js\"",
      "start": "npx tsc && node ./build/run.js",
      "debug-git": "npx tsc && GIT_TRACE_CURL=true DEBUG=simple-git:* node ./build/run.js"
   },
   "author": "Luis Weir",
   "license": "ISC",
   "dependencies": {
      "@types/node": "*",
      "@types/ws": "^8.5.3",
      "dotenv": "*",
      "got": "*",
      "graphql-ws": "*",
      "ws": "^8.9.0"
   },
   "devDependencies": {
      "@typescript-eslint/eslint-plugin": "*",
      "@typescript-eslint/parser": "*",
      "eslint": "*",
      "ts-node": "*",
      "tsc-watch": "*",
      "typescript": "*"
   }
}
